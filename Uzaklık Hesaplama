import math
print("İki nokta arasındaki uzaklık:")
Birinci_Apsis=float(input("Birinci Apsis:"))
İkinci_Apsis=float(input("İkinci Apsis:"))
Birinci_Ordinat=float(input("Birinci Ordinat:"))
İkinci_Ordinat=float(input("İkinci Ordinat:"))
a=((Birinci_Apsis - İkinci_Apsis)**2) + ((Birinci_Ordinat - İkinci_Ordinat)**2)
print("İki nokta arası uzaklık:" , math.sqrt(a) )
